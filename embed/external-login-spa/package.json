{
  "name": "@divinci-ai/external-login-spa",
  "version": "1.0.0",
  "private": true,
  "description": "SPA example for external login with Divinci embed",
  "scripts": {
    "build": "webpack --mode production",
    "build:watch": "webpack --mode development --watch",
    "server": "nodemon server/index.js",
    "dev": "concurrently \"npm run build:watch\" \"npm run server\"",
    "start": "npm run build && node server/index.js",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@divinci-ai/embed-script": "file:../../embed-script",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "jsonwebtoken": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-react": "^7.18.0",
    "babel-loader": "^9.1.0",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.0",
    "rimraf": "^6.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  }
}
